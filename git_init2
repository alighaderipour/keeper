# 1. clone کردن repo از گیت‌هاب
git clone https://github.com/alighaderipour/vali.git 

# 2. وارد پوشه شو
cd vali

# 3. فایل‌ها رو اضافه کن (مثل app.py, index.html و غیره)
# مثلاً یک فایل تستی بساز:
echo "print('Hello Vali')" > app.py

# 4. فایل‌ها رو آماده کن
git add .

# 5. commit کن
git commit -m "initial commit"

# 6. push کن
git push -u origin main
